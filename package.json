    "test:coverage": "cross-env CI=true react-app-rewired test --env=jsdom --no-cache --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "format": "prettier --write \"./**/*.{js,jsx}\""
