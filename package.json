    "start": "react-app-rewired start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "build:dev": "dotenv -e .\\.env.development -e .\\.env -- react-app-rewired build",
    "build:aws-dev": "dotenv -e .\\.env.aws.development -e .\\.env -- react-app-rewired build",
    "build:default": "react-app-rewired build",
    "test": "cross-env CI=true react-app-rewired test --env=jsdom",
